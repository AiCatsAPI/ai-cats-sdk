{
  "name": "@aicats/sdk",
  "version": "1.0.4",
  "description": "Official JavaScript SDK for the ai-cats.net API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "public": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AiCatsAPI/ai-cats-sdk.git"
  },
  "keywords": [
    "ai",
    "cats",
    "api",
    "sdk",
    "ai-cats",
    "images"
  ],
  "author": {
    "name": "Mario Bertsch",
    "url": "https://mariobertsch.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AiCatsAPI/ai-cats-sdk/issues"
  },
  "homepage": "https://ai-cats.net",
  "devDependencies": {
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
